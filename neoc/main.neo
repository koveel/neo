printf :: (msg: *i8);

print :: (msg: *i8)
{
    printf(msg);
    printf("\n");
}

main :: () -> i32
{
    nums := [] {
        0, 1, 2, 3, 4
    };
    msgs := [] {
        "one", "two", "three"
    };

    msg := @(msgs[0]);
    print(*(msg + 2));

    num := @nums[0];
    num += 2;
    return *(num - 1);
}